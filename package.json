{
    "name": "slack-in-your-time",
    "version": "0.0.1",
    "description": "A Slack application that can convert your time to everyone else's",
    "author": "Hoon Kim",
    "scripts": {
        "test": "NODE_ENV=test jest --verbose --coverage && eslint '*/**/*.{js,ts}'",
        "start": "ts-node -r dotenv/config src/index.ts",
        "dev": "ts-node-dev -r dotenv/config src/index.ts",
        "lint": "eslint '*/**/*.{js,ts}' --quiet --fix"
    },
    "keywords": [
        "slack-app"
    ],
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^26.0.15",
        "@types/lodash": "^4.14.161",
        "@types/node": "^14.14.2",
        "@types/node-fetch": "^2.5.7",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "@vercel/ncc": "^0.24.1",
        "dotenv": "^8.2.0",
        "eslint": "^7.12.0",
        "eslint-config-prettier": "^6.14.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^26.6.1",
        "jest-config": "^26.6.1",
        "prettier": "^2.1.2",
        "rimraf": "^3.0.2",
        "sync-glitch-cli": "^2.0.1",
        "ts-jest": "^26.4.2",
        "ts-node-dev": "^1.0.0-pre.63",
        "typescript": "^4.0.3"
    },
    "dependencies": {
        "@slack/bolt": "^2.3.0",
        "axios": "^0.20.0",
        "lodash": "^4.17.20",
        "moment": "^2.29.0",
        "node": "^15.0.1",
        "ts-node": "^8.6.2",
        "reflect-metadata": "^0.1.13"
    },
    "engines": {
        "node": ">=12.x"
    },
    "standard": {
        "parser": "@typescript-eslint/parser",
        "env": [
            "jest"
        ],
        "plugins": [
            "typescript"
        ]
    },
    "jest": {
        "testEnvironment": "node"
    },
    "private": false
}
