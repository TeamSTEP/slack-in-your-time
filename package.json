{
    "name": "slack-in-your-time",
    "version": "0.0.1",
    "description": "A Slack application that can convert your time to everyone else's",
    "author": "Hoon Kim",
    "main": "index.ts",
    "scripts": {
        "test": "NODE_ENV=test jest --verbose --coverage",
        "test:watch": "NODE_ENV=test jest --verbose --coverage --watchAll",
        "serve": "NODE_ENV=development nodemon --exec 'ts-node' src/index.ts",
        "start": "NODE_ENV=production ts-node src/index.ts"
    },
    "keywords": [
        "slack-app",
        "github",
        "global-work"
    ],
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^24.0.18",
        "@types/node": "^12.7.4",
        "dotenv": "^8.2.0",
        "jest": "^24.9.0",
        "nodemon": "^1.19.2",
        "prettier": "^2.0.5",
        "ts-jest": "^24.0.2",
        "ts-node": "^9.0.0",
        "tslint": "^6.1.2",
        "typescript": "^4.0.2"
    },
    "dependencies": {
        "@slack/bolt": "^2.3.0",
        "axios": "^0.20.0"
    },
    "standard": {
        "parser": "@typescript-eslint/parser",
        "env": [
            "jest"
        ],
        "plugins": [
            "typescript"
        ]
    },
    "jest": {
        "testEnvironment": "node"
    },
    "private": false
}
